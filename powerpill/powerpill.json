{
  "aria2": {
    "args": [
        "--conf-path=/home/pawel/.config/aria2/aria2.powerpill"
    ],
    "path": "/usr/bin/aria2c"
  },
  "pacman": {
    "config": "/etc/pacman.conf",
    "path": "/usr/bin/pacman"
  },
  "pacserve": {
    "server": null
  },
  "powerpill": {
    "select": true,
    "reflect databases": false
  },
  "reflector": {
    "args.unsued": [
      "--protocol", "http",
      "--latest", "50"
    ]
  },
  "rsync": {
    "args": [
      "--no-motd",
      "--progress"
    ],
    "db only": true,
    "path": "/usr/bin/rsync",
    "servers": [
        "rsync://mirror.f4st.host/archlinux/$repo/os/$arch",
        "rsync://ftp.halifax.rwth-aachen.de/archlinux/$repo/os/$arch",
        "rsync://mirror.ams1.nl.leaseweb.net/archlinux/$repo/os/$arch",
        "rsync://ftp.nluug.nl/archlinux/$repo/os/$arch",
        "rsync://mirror.dkm.cz/archlinux/$repo/os/$arch",
        "rsync://repo.itmettke.de/archlinux/$repo/os/$arch",
        "rsync://mirror.pseudoform.org/packages/$repo/os/$arch",
        "rsync://mirror.selfnet.de/archlinux/$repo/os/$arch",
        "rsync://rsync.osbeck.com/archlinux/$repo/os/$arch",
        "rsync://ftp.sh.cvut.cz/arch/$repo/os/$arch"
    ]
  }
}
